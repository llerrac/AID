  object qryBOM: TcrIBQuery
    Params = <>
    RecordCountAccurate = True
    SQL.Strings = (
      'SELECT'
      '  bm.Inc_BOM,'
      '  bm.BOMNo, bm.Issue, bm.BOMDesc,'
      '  bm.IsObsolete, bm.Model,  bm.ModelLite, bm.HasOptions,'
      '  bm.Key_Eng, bm.Key_EngLite, ba.Assembly as assy, bm.DteAdded,'
      '  bm.BOMNote, bm.BOMAka1, '
      '  bm.CheckOutState, '
      '  bm.CheckedOutBy, um.UserLong as CheckedOutByName, '
      '  bm.CheckedOutAt,  bm.CheckOutReason'
      'FROM'
      '  BOM bm'
      'LEFT JOIN Users um ON (bm.CheckedOutBy = um.UserName)'
      
        'LEFT JOIN Proc_Bom_GetAssyInfo( bm.BomNo, bm.DrawingInfo ) ba on' +
        ' ( 1 = 1 )'
      'ORDER BY'
      '  bm.BOMNo')
    FieldOptions = []
    OptionsEx = []
    Left = 155
    Top = 5
